<div class="profile-navigation">
  <%= link_to events_path, class: "nav-icon left" do %>
    <i class="fa-solid fa-chevron-left"></i>
  <% end %>
  <%# <h3 class="profile-title">My challenges</h3> %>
</div>

<div class="container">
  <div class="title-container">
    <h2 class="challenge-title">My Challenges</h2>
  </div>
</div>

<div class="container users-list">
  <div class="dashboard-container">
    <% if @challenges.any? %>
      <% @challenges.each do |challenge| %>
        <%= render "challenges/challenge", challenge: challenge %>
      <% end %>
    <% else %>
     <%= render "challenges/new_challenge"%>
    <% end %>
  </div>
</div>
